{
    "name": "gungnir-mvc/asset",
    "description": "Asset library for gungnir-mvc",
    "type": "library",
    "require": {
      "gungnir-mvc/core": "dev-master",
      "gungnir-mvc/http": "v1.*",
      "gungnir-mvc/framework": "dev-master",
      "gungnir-mvc/event": "dev-master"
    },
        "require-dev": {
                "phpunit/phpunit": "7.0.0",
                "phpspec/prophecy": "~1.0",
                "mikey179/vfsStream": "~1.6"
                },
        "autoload": {
        "psr-4": { "Gungnir\\Asset\\": "src/" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gungnir-mvc/core.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/gungnir-mvc/http.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/gungnir-mvc/framework.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/gungnir-mvc/event.git"
    }
  ]
}
